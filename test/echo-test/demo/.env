# This .env is loaded from your working directory (PWD).
# It also points Compose to the real project file in demo/.
COMPOSE_FILE=compose.yaml
COMPOSE_PROJECT_NAME=project_demo

# NOT WORKS. It should be defined before run docker compose
COMPOSE_ENV_FILES=.env,.env.test

# This .env is loaded from the project directory (demo/)
# Lower precedence when you run from workspace root using COMPOSE_FILE.
SOURCE=FROM_DEMO_DOTENV
ONLY_IN_PROJECT=PROJECT_ONLY
