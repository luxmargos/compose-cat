# This .env is loaded from your working directory (PWD).
# It also points Compose to the real project file in demo/.
COMPOSE_FILE=demo/compose.yaml
COMPOSE_PROJECT_NAME=project_root_demo
COMPOSE_ENV_FILES=.env,.env.test

# High precedence when you run Compose from this folder
SOURCE=FROM_ROOT_DOTENV
ONLY_IN_ROOT=ROOT_ONLY
